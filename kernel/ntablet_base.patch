+/dts-v1/;
+#include <dt-bindings/clock/rockchip,rk808.h>
+#include <dt-bindings/pwm/pwm.h>
+#include <dt-bindings/input/input.h>
+#include <dt-bindings/display/media-bus-format.h>
+#include <dt-bindings/soc/rockchip-system-status.h>
